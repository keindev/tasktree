{
    "name": "tasktree-cli",
    "version": "2.0.2",
    "description": "Simple terminal task tree - helps you keep track of your tasks in a tree structure.",
    "homepage": "https://github.com/keindev/tasktree#readme",
    "license": "MIT",
    "author": "Daniil Ryazanov <opensource@tagproject.ru>",
    "keywords": [
        "cli",
        "task",
        "tree",
        "terminal",
        "term",
        "console",
        "ascii",
        "loading",
        "indicator",
        "progress",
        "busy",
        "wait",
        "idle"
    ],
    "main": "lib/TaskTree.js",
    "types": "lib/TaskTree.d.ts",
    "module": "lib/TaskTree.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/keindev/tasktree.git"
    },
    "bugs": {
        "url": "https://github.com/keindev/tasktree/issues"
    },
    "scripts": {
        "clean": "rimraf rmdir lib coverage",
        "lint": "eslint src/**/*.ts",
        "spellcheck": "cspell -c .vscode/cspell.json --no-summary \"*.*\" \"docs/**/*\" \"**/*.ts\" \"**/*.js\"",
        "test": "jest",
        "changelog": "changelog generate",
        "build:ts": "tsc",
        "build": "npm-run-all clean spellcheck lint build:* test"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "dependencies": {
        "chalk": "^3.0.0",
        "color-convert": "^2.0.1",
        "elegant-spinner": "^2.0.0",
        "figures": "^3.1.0",
        "stdout-update": "^1.3.7"
    },
    "devDependencies": {
        "@types/color-convert": "^1.9.0",
        "@types/jest": "^24.0.23",
        "@types/node": "^12.12.8",
        "@typescript-eslint/eslint-plugin": "^2.7.0",
        "@typescript-eslint/parser": "^2.7.0",
        "changelog-guru": "^1.0.0",
        "cspell": "^4.0.33",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.20.0",
        "eslint-plugin-jest": "^23.6.0",
        "husky": "^3.0.9",
        "jest": "^24.9.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.0",
        "strip-ansi": "^6.0.0",
        "ts-jest": "^24.1.0",
        "typescript": "^3.7.2"
    }
}
